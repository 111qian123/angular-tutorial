<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>

	<body>
		<script>
			//创建表格
			var table=document.createElement("table")
			table.width=300;
			table.border=1;
			//添加表格的头
			var tr=document.createElement('tr')
			table.appendChild(tr)
			var th1=document.createElement('th')
			th1.appendChild(document.createTextNode("ccc"));
			var th2=document.createElement('th')
			th2.appendChild(document.createTextNode("ccc"));
			tr.appendChild(th1);
			tr.appendChild(th2);
			//输出表格
			document.body.appendChild(table);
		</script>
	</body>

</html>
